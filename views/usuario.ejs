<!DOCTYPE html>
<html lang="es">
<head>
  <meta charset="UTF-8">
  <title>Lista de Usuarios</title>
</head>
<body>
  <h1>Usuarios</h1>
  <table border="1">
    <tr>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Correo</th>
    </tr>
    <% usuarios.forEach(usuario => { %>
      <tr>
        <td><%= usuario.pnombre %></td>
        <td><%= usuario.appaterno %></td>
        <td><%= usuario.correo %></td>

        <td><%= usuario.contrasena %></td>
        <td><%= usuario.id_rol %></td>

      </tr>
    <% }) %>
  </table>
</body>
</html>
